{
  "name": "muizznwali",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python13 -m uvicorn api.index:app --reload",
    "fastapi-dev-win": "python -m pip install -r requirements.txt && python -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "dev:all-win": "concurrently \"npm run next-dev\" \"npm run fastapi-dev-win\"",
    "build": "next build --turbopack",
    "build:prod": "next build",
    "start": "next start",
    "lint": "eslint",
 "clear-cache": "rimraf .next node_modules/.cache",
    "clean": "npm run clear-cache && npm install",
    "python-install": "pip install -r requirements.txt",
    "python-venv-win": "python -m venv .venv && .venv\\Scripts\\pip.exe install -r requirements.txt",
    "python-venv": "python3 -m venv .venv && .venv/bin/pip install -r requirements.txt",
    "python-dev": "python src/app/api/main.py",
    "vercel-deploy": "vercel --prod",
    "vercel-dev": "vercel dev",
    "build-all": "npm run python-install && npm run build"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "firebase": "^12.2.1",
    "next": "15.5.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "@ai-sdk/ui-utils": "^0.0.20",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@vercel/kv": "^2.0.0",
    "ai": "^4.0.2",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.1",
    "date-fns": "^4.1.0",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "framer-motion": "^11.11.17",
    "geist": "^1.3.1",
    "lucide-react": "^0.460.0",
    "postcss": "8.4.23",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.0.4",
    "usehooks-ts": "^3.1.0",
    "rimraf": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}